{
    "name": "sierratecnologia/trainner",
    "description": "trainner",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "sierratecnologia/finder": "dev-master",
        "sierratecnologia/gamer": "dev-master",
        "sierratecnologia/casa": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Trainner\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Trainner\\TrainnerProvider"
            ],
            "dont-discover": [
                "laravel/cashier"
            ]
        }
    }
}
